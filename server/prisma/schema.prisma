datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Question {
  id        Int      @id @default(autoincrement())
  content   String
  createdAt DateTime @default(now())
  tag       String
}

model User {
  id        Int      @id @default(autoincrement())
  //name      String
  mail     String   @unique
  password  String
  createdAt DateTime @default(now())
  faculty    String
}
